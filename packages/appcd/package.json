{
  "name": "appcd",
  "version": "4.0.2",
  "description": "A daemon that powers Appcelerator tooling and makes the impossible possible.",
  "author": "Axway, Inc. <npmjs@appcelerator.com>",
  "maintainers": [
    "Chris Barber <cbarber@axway.com>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "appcelerator",
    "appc",
    "daemon",
    "amplify-package"
  ],
  "bin": {
    "appcd": "./bin/appcd"
  },
  "main": "./dist/main",
  "preferGlobal": true,
  "scripts": {
    "build": "gulp build",
    "coverage": "gulp coverage",
    "docs": "gulp docs",
    "package": "gulp package",
    "postinstall": "node scripts/postinstall.js || node -e true",
    "prepare": "gulp build",
    "test": "gulp test",
    "uninstall": "node scripts/uninstall.js || node -e true"
  },
  "dependencies": {
    "appcd-client": "^3.0.5",
    "appcd-core": "^4.2.3",
    "appcd-dump-viewer": "^1.1.4",
    "appcd-fs": "^2.0.5",
    "appcd-logger": "^3.0.5",
    "appcd-nodejs": "^4.1.4",
    "appcd-path": "^2.0.6",
    "appcd-util": "^3.1.4",
    "boxen": "^5.0.1",
    "check-kit": "^1.1.2",
    "cli-kit": "^1.11.3",
    "cli-table3": "^0.6.0",
    "global-prefix": "^3.0.0",
    "humanize": "^0.0.9",
    "infer-owner": "^1.0.4",
    "pretty-ms": "^7.0.1",
    "semver": "^7.3.5",
    "source-map-support": "^0.5.19",
    "v8-compile-cache": "^2.3.0"
  },
  "devDependencies": {
    "appcd-gulp": "^3.1.4"
  },
  "homepage": "https://github.com/appcelerator/appc-daemon",
  "bugs": "https://github.com/appcelerator/appc-daemon/issues",
  "repository": "https://github.com/appcelerator/appc-daemon/tree/master/packages/appcd",
  "engines": {
    "node": ">=10.19.0"
  },
  "cli-kit": {
    "description": "The Appc Daemon for powering client-side tooling.",
    "main": "./dist/cli"
  },
  "amplify": {
    "type": "amplify-cli-plugin"
  }
}
