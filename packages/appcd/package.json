{
  "name": "appcd",
  "version": "3.0.0",
  "description": "A daemon that powers Appcelerator tooling and makes the impossible possible.",
  "author": "Axway, Inc. <npmjs@appcelerator.com>",
  "maintainers": [
    "Chris Barber <cbarber@axway.com>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "appcelerator",
    "appc",
    "daemon",
    "amplify-package"
  ],
  "bin": {
    "appcd": "./bin/appcd"
  },
  "main": "./dist/main",
  "preferGlobal": true,
  "scripts": {
    "build": "gulp build",
    "coverage": "gulp coverage",
    "docs": "gulp docs",
    "package": "gulp package",
    "postinstall": "node bin/appcd stop || node -e true",
    "prepare": "gulp build",
    "test": "gulp test"
  },
  "dependencies": {
    "appcd-client": "^2.0.1",
    "appcd-core": "^3.0.0",
    "appcd-dump-viewer": "^1.1.4",
    "appcd-fs": "^1.1.8",
    "appcd-logger": "^2.0.3",
    "appcd-nodejs": "^3.0.0",
    "appcd-path": "^1.1.7",
    "appcd-util": "^2.0.0",
    "cli-kit": "^0.12.0",
    "cli-table2": "^0.2.0",
    "humanize": "^0.0.9",
    "source-map-support": "^0.5.13",
    "v8-compile-cache": "^2.1.0"
  },
  "devDependencies": {
    "appcd-gulp": "^2.2.0"
  },
  "homepage": "https://github.com/appcelerator/appc-daemon",
  "bugs": "https://github.com/appcelerator/appc-daemon/issues",
  "repository": "https://github.com/appcelerator/appc-daemon/tree/master/packages/appcd",
  "engines": {
    "node": "^8.12.0 || >=10.2.0"
  },
  "cli-kit": {
    "description": "The Appc Daemon for powering client-side tooling.",
    "main": "./dist/cli"
  },
  "amplify": {
    "type": "amplify-cli-plugin"
  }
}
